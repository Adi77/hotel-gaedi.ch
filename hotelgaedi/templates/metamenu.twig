<!-- meta menu -->

{% if metamenu %}
<ul>
  {% for item in items %}
  <li class="{{ item.classes | join(' ') }}">
    <a target="{{ item.target }}" href="{{ item.link }}">{{ item.title }}</a>
    {# {% include "metamenu.twig" with {'items': item.children} %} #}
  </li>
  {% endfor %}
</ul>
{% endif %}
